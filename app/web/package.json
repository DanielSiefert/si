{
  "name": "web",
  "version": "0.0.0",
  "scripts": {
    "clean": "rm -rf ./dist ./lib ./target",
    "build": "vite build",
    "build:check": "vue-tsc --noEmit",
    "build:clean": "npm run clean && npm run build",
    "build:watch": "npm run clean && tsc --watch",
    "build:pedantically": "npm run build:check && npm run build",
    "watch": "npm run build:watch",
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore src",
    "lint:fix": "npm run lint -- --fix",
    "lint:strict": "STRICT_LINT=1 npm run lint",
    "fmt": "prettier --write ./src",
    "fmt:check": "prettier --check ./src",
    "check": "npm run lint && npm run build:check && npm run fmt:check",
    "dev": "npm run start",
    "start": "vite",
    "serve": "vite preview --port 8080",
    "test": "echo 'Sorry, no tests yet!'",
    "vite:clean": "./script/clean-vite-cache.sh",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open"
  },
