{
  "compilerOptions": {
    "lib": [
      "es5",
      "dom"
    ],
    "types": [
      "cypress",
      "node",
      "vite/client"
    ],
    "isolatedModules": false,
    "allowJs": true,
    "noEmit": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "target": "es5",
  },
  "include": [
    "**/*.ts",
    "../src/**/*.ts",
    "../cypress.d.ts"
  ],
  "extends": "../tsconfig.json",
}